   "source": [
    "## Q1. What is the total amount each customer spent at the restaurant?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "SELECT a.customer_id,\n",
    "       sum(b.price) as amt\n",
    "FROM dannys_diner.sales a\n",
    "join dannys_diner.menu b\n",
    "on a.product_id = b.product_id\n",
    "group by 1\n",
    "ORDER BY amt DESC;"
   ]
  },
